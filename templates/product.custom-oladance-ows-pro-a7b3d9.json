      "type": "custom-hero-ksp-a7b3d9",
      "settings": {
        "heading": "Stable enough to stay put, comfortable enough to forget.",
        "subheading": "<p>Our patented open ear design and integrated liquid silicone molding mean the OWS Pro rests gently on top of your ear, never intruding. Comfort all day, every day.</p>",
        "cta_text": "Shop Oladance OWS Pro"
      }
    },
    "custom_social_proof_a7b3d9": {
      "type": "custom-social-proof-a7b3d9",
      "blocks": {
        "testimonial_1": {
          "type": "testimonial",
          "settings": {
            "quote": "I'm a road cyclist and these are a game changer. I can hear traffic and still enjoy my music. Super comfortable and secure.",
            "author": "Mike R.",
            "author_meta": "Road Cyclist",
            "rating": 5
          }
        },
        "testimonial_2": {
          "type": "testimonial",
          "settings": {
            "quote": "Perfect for my commute and office. I can stay aware of my surroundings while focusing on my work. The battery life is insane!",
            "author": "Jessica P.",
            "author_meta": "Daily Commuter",
            "rating": 5
          }
        },
        "testimonial_3": {
          "type": "testimonial",
          "settings": {
            "quote": "As a runner, I've struggled to find headphones that don't fall out or block out important sounds. The OWS Pro solves both problems. Highly recommend!",
            "author": "David Chen",
            "author_meta": "Fitness Enthusiast",
            "rating": 5
          }
        }
      },
      "block_order": ["testimonial_1", "testimonial_2", "testimonial_3"],
      "settings": {
        "heading": "Trusted by Athletes & Professionals"
      }
