    "custom_feature_highlights_a7b3d9": {
      "type": "custom-feature-highlights-a7b3d9",
      "blocks": {
        "feature_1": {
          "type": "feature",
          "settings": {
            "title": "The second-generation bass algorithm",
            "description": "<p>Our patented Virtual Bass 2nd algorithm uses high dynamic power to enhance low-frequency sound quality, significantly improving bass volume for a fuller, deeper bass.</p>"
          }
        },
        "feature_2": {
          "type": "feature",
          "settings": {
            "title": "58 hours of full battery life",
            "description": "<p>Our self-developed 150mAh 'Little Steel Cannon' battery offers high density, small volume, stronger charge storage, and higher safety.</p>"
          }
        },
        "feature_3": {
          "type": "feature",
          "settings": {
            "title": "Stay aware of every sound of nature",
            "description": "<p>With our patented open ear design, the OWS Pro keeps you in sync with your surroundings, without sealing the ear canal.</p>"
          }
        }
      },
      "block_order": ["feature_1", "feature_2", "feature_3"],
      "settings": {
